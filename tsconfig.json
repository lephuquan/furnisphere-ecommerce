{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "jsx": "react",
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true
  },
  "include": ["src", "client/src/components/common/Header/CategoryNav.tsx", "client/src/components/common/Header/Header.tsx", "client/src/components/common/Header/MainNav.tsx", "client/src/components/common/Header/TopNav.tsx", "client/src/styles/variables.ts"]
} 